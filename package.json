{
  "name": "yocto-erp",
  "version": "1.0.0",
  "description": "A simple erp using for small company.",
  "repository": {
    "type": "git",
    "url": "https://github.com/yocto-erp/yocto-erp-web"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
